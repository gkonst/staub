<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
              version="1.2">
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
    </application>
    <component>
        <component-type>ru.spbspu.HtmlSelectBooleanRadio</component-type>
        <component-class>ru.spbspu.staub.components.html.HtmlSelectBooleanRadio</component-class>
    </component>
    <render-kit>
        <renderer>
            <component-family>javax.faces.SelectBoolean</component-family>
            <renderer-type>ru.spbspu.BooleanRadio</renderer-type>
            <renderer-class>ru.spbspu.staub.components.renderkit.BooleanRadioRenderer</renderer-class>
        </renderer>
    </render-kit>
    <validator>
        <validator-id>ru.spbspu.staub.RegexValidator</validator-id>
        <validator-class>ru.spbspu.staub.components.validator.RegexValidator</validator-class>
    </validator>
    <converter>
        <converter-id>ru.spbspu.staub.ObjectSelectConverter</converter-id>
        <converter-class>ru.spbspu.staub.components.converter.ObjectSelectConverter</converter-class>
    </converter>
</faces-config>