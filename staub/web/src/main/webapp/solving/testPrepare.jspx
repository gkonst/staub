<!DOCTYPE html
        PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:s="http://jboss.com/products/seam/taglib">
<ui:composition template="/template/staub-template.jspx">
    <!--@elvariable id="messages" type="java.util.Map"-->
    <ui:define name="pageScript">
        <script type="text/javascript">
            function onBodyLoad() {
                setTimeout('document.getElementById(\'testPrepareButton\').click();', 2000);
            }
        </script>
    </ui:define>
    <ui:define name="contentTitle">
        #{messages['test.prepare.title']}
    </ui:define>
    <ui:define name="contentBody">
        #{messages['test.prepare.message']}
        <s:button id="testPrepareButton"
                  view="/question/question.seam"
                  propagation="join"
                  style="display:none;">
            <f:param name="beanMode" value="VIEW_MODE"/>
        </s:button>
    </ui:define>
</ui:composition>
</html>