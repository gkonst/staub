<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
              version="1.2">
    <application>
        <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
    </application>

    <navigation-rule>
       <from-view-id>/task/startTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>next</from-outcome>
          <to-view-id>/task/radioTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>/task/radioTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>end</from-outcome>
          <to-view-id>/task/endTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>/task/radioTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>prev</from-outcome>
          <to-view-id>/task/startTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
       <from-view-id>/task/radioTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>next</from-outcome>
          <to-view-id>/task/imageTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>/task/imageTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>end</from-outcome>
          <to-view-id>/task/endTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>/task/imageTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>prev</from-outcome>
          <to-view-id>/task/radioTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
       <from-view-id>/task/imageTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>next</from-outcome>
          <to-view-id>/task/inputTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>/task/inputTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>end</from-outcome>
          <to-view-id>/task/endTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>/task/inputTask.jspx</from-view-id>
        <navigation-case>
          <from-outcome>prev</from-outcome>
          <to-view-id>/task/imageTask.jspx</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>