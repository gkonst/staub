<!DOCTYPE html
        PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:s="http://jboss.com/products/seam/taglib">
<ui:composition template="../template/staub-template.jspx">
    <ui:param name="pageBean" value="#{testEndBean}"/>
    <!--@elvariable id="pageBean" type="ru.spbspu.staub.bean.test.TestEndBean"-->
    <!--@elvariable id="messages" type="java.util.Map"-->
    <ui:define name="contentTitle">
        Результаты теста.
    </ui:define>
    <ui:define name="contentBody">
        <table class="detailTable">
            <tr>
                <td align="right">
                    <strong>Название теста</strong>
                </td>
                <td>
                    #{pageBean.model.test.name}
                </td>
            </tr>
            <tr>
                <td align="right">
                    <strong>Дата начала теста</strong>
                </td>
                <td>
                    #{pageBean.model.started}
                </td>
            </tr>
            <tr>
                <td align="right">
                    <strong>Дата окончания теста</strong>
                </td>
                <td>
                    #{pageBean.model.finished}
                </td>
            </tr>
            <tr>
                <td align="right">
                    <strong>Кол-во баллов</strong>
                </td>
                <td>
                    #{pageBean.model.score}/#{pageBean.model.test.passScore}
                </td>
            </tr>
            <tr>
                <td align="right">
                    <strong>Результат</strong>
                </td>
                <td>
                   #{pageBean.model.testPassed ? 'Сдан' : 'Не сдан'}
                </td>
            </tr>
        </table>
    </ui:define>
</ui:composition>
</html>