<!DOCTYPE html
        PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:s="http://jboss.com/products/seam/taglib">
<ui:composition template="../template/staub-template.jspx">
    <ui:param name="pageBean" value="#{questionListBean}"/>
    <ui:define name="contentTitle">
        Список вопросов
    </ui:define>
    <ui:define name="contentBody">
        <div class="section">
            <ui:decorate template="../controls/listLayoutControl.jspx">
                <ui:define name="listPageData">
                    <h:outputText value="#{messages['common.list.noDataFound']}"
                                  rendered="#{dataModel == null or dataModel.rowCount == 0}"/>
                    <h:dataTable id="questionList"
                                 value="#{dataModel}"
                                 var="question"
                                 rendered="#{dataModel.rowCount > 0}"
                                 styleClass="dataTable">
                        <h:column>
                            <f:facet name="header">aa</f:facet>
                            aaa
                        </h:column>
                        <h:column>
                            <f:facet name="header">bbb</f:facet>
                            bbbb
                        </h:column>
                    </h:dataTable>
                </ui:define>
            </ui:decorate>
        </div>
    </ui:define>
</ui:composition>
</html>